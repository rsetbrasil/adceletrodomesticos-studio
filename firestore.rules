rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    // Allows anyone to read and write to the database.
    // This is an open rule, ideal for prototyping and initial development.
    // Before launching your application to the public, you should implement
    // stricter security rules.
    match /{document=**} {
      allow read, write: if true;
    }
  }
}
